@model C4FAMS.Models.DangKySuKien

<h1>Xóa Đăng ký Sự kiện</h1>

<h3>Bạn có chắc muốn xóa đăng ký này?</h3>
<div>
    <h4>@Model.CuuSinhVien.SinhVien.HoTen - @Model.SuKien.TenSuKien</h4>
    <form asp-action="Delete" method="post">
        <input type="hidden" asp-for="MaDangKy" />
        <input type="submit" value="Xóa" class="btn btn-danger" /> |
        <a asp-action="Index">Quay lại</a>
    </form>
</div>